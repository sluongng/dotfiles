{ 
  "suggest.autoTrigger": "always",
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "gosnippets/UltiSnips"
  ],
	"languageserver": {
		"golang": {
			"trace.server": "verbose",
		  "command": "gopls",
		  "rootPatterns": ["go.mod", ".git/"],
		  "filetypes": ["go"]
		},

		"dockerfile": {
			"command": "docker-langserver",
			"filetypes": ["dockerfile"],
			"args": ["--stdio"]
		}
	}
}
