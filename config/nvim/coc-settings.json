{
  "suggest.autoTrigger": "always",
  "suggest.floatEnable": true,
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "gosnippets/UltiSnips"
  ],
  "python.formatting.provider": "black",
  "json.format.enable": true,
  "go.goplsPath": "/Users/sngoc/work/golang/bin/gopls",
  "go.goplsOptions": {
    "directoryFilters": [
      "-bazel-bin",
      "-bazel-out",
      "-bazel-testlogs"
    ]
  },
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": [
        "dockerfile"
      ],
      "args": [
        "--stdio"
      ]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": [
        "serve"
      ],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "java.enabled": false
}
