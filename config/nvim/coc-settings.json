{
	"suggest.autoTrigger": "always",
	"snippets.ultisnips.directories": [
		"UltiSnips",
		"gosnippets/UltiSnips"
	],
	"json.format.enable": true,
	"go.tags": {
		"tags": "linux"
	},
	"go.goplsPath": "/Users/sluongng/work/golang/bin/gopls",
	"go.goplsOptions": {
		"directoryFilters": [
			"-bazel-bin",
			"-bazel-out",
			"-bazel-testlogs"
		]
	},
	"languageserver": {
		"dockerfile": {
			"command": "docker-langserver",
			"args": [
				"--stdio"
			],
			"filetypes": [
				"dockerfile"
			]
		},
		"starlark": {
			"command": "starpls",
			"args": [],
			"filetypes": [
				"bzl"
			]
		},
		"protobuf": {
			"command": "bufls",
			"args": [
				"serve"
			],
			"filetypes": [
				"proto",
			],
			"initializationOptions": {},
			"settings": {}
		},
		"terraform": {
			"command": "terraform-ls",
			"args": [
				"serve"
			],
			"filetypes": [
				"terraform",
				"tf"
			],
			"initializationOptions": {},
			"settings": {}
		}
	},
	"java.enabled": false,
	"snippets.ultisnips.pythonPrompt": false
}
